<html lang="en">
  <head>
    {% block head %}
    <title>API | Giovanni</title>
    <meta name="author" content="Giovanni Dwidjosewojo">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {% endblock %} {% if NODE_ENV == "development" %}
    <script type="module" src="http://localhost:3000/@vite/client" defer></script>
    <script type="module" src="http://localhost:3000/src/main.js" defer></script>
    {% else %}
    <link rel="stylesheet" href="./assets/main.css" />
    <script type="module" src="./main.js" defer></script>
    <script type="nomodule" src="/polyfills-legacy.js" defer></script>
    <script type="nomodule" src="/main-legacy.js" defer></script>
    {% endif %} {% block styles %} {% endblock %} {% block scripts %} {% endblock %}
  </head>

  <body>
    {% render './partials/nav.liquid' %}
    {% block content %} {% endblock %}
  </body>
</html>
